// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Schedule {
  id       Int    @id @unique @default(autoincrement())
  category String
  name     String
  adress   String
  contact  String
  date     String
}
